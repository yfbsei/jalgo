# Trading pair and market settings
TRADING_PAIR=BTCUSDT
TIMEFRAME=4h
MARKET_TYPE=futures  # spot or futures

# Discord webhook URLs for notifications
DISCORD_WEBHOOK=https://discord.com/api/webhooks/your-webhook-url
DISCORD_WEBHOOK_SIGNALS=
DISCORD_WEBHOOK_EXITS=
DISCORD_WEBHOOK_STATS=

# Algorithm parameters
FAST_LENGTH=6
ATR_PERIOD=16
ATR_MULTIPLIER=9
ATR_MULTIPLIER_FAST=5.1

# Scalp mode parameters
SCALP_PERIOD=21

# Risk-reward parameters
REWARD_MULTIPLE=1.5
INITIAL_CAPITAL=100
RISK_PER_TRADE=10

# Leverage parameters
USE_LEVERAGE=true
LEVERAGE_AMOUNT=2.0

# Application settings
TEMP_DIR=/tmp
CONFIG_FILE=config.json